{
  "name": "Youtube MP3 Right-Click-To-Download",
  "description": "Download MP3 from youtube video on the right-click context menu.",
  "version": "1.1.3",
  "manifest_version": 2,
  "permissions": [
    "contextMenus", "downloads", "notifications", "*://*.youtube.com/*"
  ],
  "background": {
    "scripts": [
      "script.js"
    ]
  },
  "browser_action": {
    "default_popup": "info.html"
  },
  "icons": {
    "48": "icon.png"
  }
}